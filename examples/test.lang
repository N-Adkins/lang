var fib := fn (x: int) -> int {
    var state := [0, 1];
    var i := 0;
    while (i <= x) {
        state[i % 2] = state[0] + state[1]; 
        i = i + 1;
    }
    return state[i % 2]; 
};

var bad_fib := fn (self, x: int) -> int {
    if (x <= 1) {
        return x;
    }
    return self(x-1) + self(x-2);
};

print(bad_fib(34));
