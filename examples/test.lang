var test_array := ["hello", "blah", "other", "thing"];

var print_each := fn (array: [string]) -> void {
    var iter := fn (self, array: [string], i: number) -> void {
        if (i == length(array)) {
            return;
        }
        print(array[i]);
        self(array, i+1);
        return;
    };
    iter(array, 0);
    return;
};

print_each(test_array);
